.main1 {
  top: 0px;
  position: fixed;
  height: 100%;
  width: 100%;
  background-repeat: repeat;
  background-color: #F6F6F7;
}

.content {
  margin-top: 8px;
  margin-left: 8px;
  background-color: #FFFFFF;
  border-radius: 4px;
  padding-top:20px;
  padding-left:20px;
  padding-right: 20px;
  padding-bottom: 20px;
}

.bottomGenBar {
  left: 10px;
  position: fixed;
  bottom: 10px;
  display: flex;
  height: 64px;
  border-radius: 4px;
  border: 1px solid rgba(210, 212, 215, 1);
}

.bottomGenBarButton {
  font-size: 13px;
  padding: 0px;
  margin-left: 10px;
  margin-top: 12px;
  margin-right: 0px;
  width: 40px;
  height: 40px;
  line-height: 40px;
  border-radius: 4px;
  color: #60646c;
  /* border: 2px solid rgba(41, 196, 181, 1); */
}

.bottomBarGenBarSaveButton {
  margin-top: 12px;
  padding: 0px;
  width: 80px;
  height: 40px;
  border-radius: 2px;
  font-size: 15px;
  line-height: 40px;
  color: #fff;
  background-color: rgba(48, 206, 172, 1);
}

.bottomBar {
  bottom: 20px;
  display: flex;
  width: 100%;
}

.bottomBarText {
  text-align: center;
}

.bottomBarButton {
  padding: 0px;
  margin-left:8px;
  margin-right:8px;
  margin-top:8px;
  border-radius: 4px;
  width:100%;
  height: 50px;  
  line-height: 50px;
  background:#30CEAC;
  font-size: 15px;
  color: #fff;
}

button::after {
  border: none;
}
